#include<stdio.h>
int main()
{
	int a[3][3];
	int flag1, max;
	printf("请输入3*3数组：\n");
	for (int i = 0;i <= 2;i++)
	{
		for (int j = 0;j <= 2;j++)
		{
			scanf("%1d", &a[i][j]);
		}
	}
	for (int i = 0;i <= 2;i++)
	{
		int j;
		max = a[i][0];
		flag1 = 0;
		for (j = 1;j <= 2;j++)
		{
			if (a[i][j] > max)
			{
				max = a[i][j];
				flag1 = j;
			}
		}
		if (a[0][flag1] >= a[i][flag1] && a[1][flag1] >= a[i][flag1] && a[2][flag1] >= a[i][flag1])  
			printf("第%d行中a[%d][%d]是鞍点\n", i+1, i, flag1);
		else printf("第%d行中不存在鞍点！\n", i+1);
	}
}
