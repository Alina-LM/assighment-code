#include<stdio.h>
#include<math.h>
double x1 = 0, x2 = 0, disc = 0, p = 0, q = 0;
void det0(double x, double y);
void detup(double x, double y);
void detlow(double x, double y);
int main()
{

	double a, b, c;
	scanf("%lf,%lf,%lf", &a, &b, &c);
	disc = b * b - 4 * a * c;
	if (disc == 0)
	{
		det0(a, b);
		printf("此二次方程的解为x1=x2=%.5lf\\n", x1);
	}
	else if (disc > 0)
	{
		detup(a, b);
		printf("此二次方程的解为x1=%.5lf,x2=%.5lf\\n", x1, x2);
	}
	else
	{
		detlow(a, b);
		printf("此二次方程的解为x1=%.5lf+%.5lfi,x2=%.5lf-%.5lfi\\n", p, q, p, q);
	}
}
void det0(double x, double y)
{
	x1 = x2 = (-y) / (2 * x);
}
void detup(double x, double y)
{
	x1 = (-y + sqrt(disc)) / (2 * x);
	x2 = (-y - sqrt(disc)) / (2 * x);
}
void detlow(double x, double y)
{
	p = -y / (2 * x);
	q = sqrt(-disc) / (2 * x);
}
