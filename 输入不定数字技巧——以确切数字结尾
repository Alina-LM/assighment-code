*/
#include <stdio.h>

/*
  以下为示例代码，仅供参考。请根据题目要求定义好方法及参数。
  The following is sample code for reference only.
  Please define the methods and parameters according to the requirements of the question.
*/

int main() {
	/*
	  在这⾥写代码
	  please write your code here
	*/
	int a[101];
	for (int i = 0;i <= 100;i++)
	{
		scanf("%d", &a[i]);
		if (a[i] == -1)break;
	}
	int s[10] = { 0 };
	for (int i = 0;i <= 100;i++)
	{
		switch (a[i])
		{
		case 0:s[0]++;break;
		case 1:s[1]++;break;
		case 2:s[2]++;break;
		case 3:s[3]++;break;
		case 4:s[4]++;break;
		case 5:s[5]++;break;
		case 6:s[6]++;break;
		case 7:s[7]++;break;
		case 8:s[8]++;break;
		case 9:s[9]++;break;
		default:continue;
		}
	}
	for (int i = 0;i <= 9;i++)
	{
		printf("%d:%d\n", i, s[i]);
	}
	//请注意：main函数必须返回0，否则无法通过评测
	return 0;
}
