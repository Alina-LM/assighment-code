#include<stdio.h>
//需要引入的文件
//如提示运行通过，但解题失败，认真对比预期输出和实际输出之间的差异
int main()
{
    //注意：main函数必须return 0,否则无法通过评测， 因此不要修改下面的代码
    int a[10];
    int i, j, minIndex, t;

    // 输入10个整数
    for (i = 0; i < 10; i++)
    {
        scanf("%d,", &a[i]);
    }
    for (i = 0; i < 9; i++)
    {  // 外层循环：从0到8（共9轮）
        minIndex = i;  // 假设当前i位置是最小值

        // 内层循环：在未排序部分查找真正的最小值
        for (j = i + 1; j < 10; j++)
        {
            if (a[j] < a[minIndex])
            {
                minIndex = j;  // 更新最小值的索引
            }
        }

        // 如果找到的最小值不在当前位置，则交换
        if (minIndex != i) {
            t = a[i];
            a[i] = a[minIndex];
            a[minIndex] = t;
        }
    }

    // 显示最终排序结果
    for (i = 0; i < 10; i++)
    {
        printf("%d", a[i]);
    }
    return 0;
}
